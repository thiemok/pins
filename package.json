{
  "name": "pins",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "bun x vercel env pull && bun run next dev",
    "build": "bun run next build",
    "start": "bun run next start",
    "lint": "bun run --bun next lint",
    "db:generate": "dotenvx run -f .env.development.local -f .env.local -- drizzle-kit generate",
    "db:migrate": "dotenvx run -f .env.development.local -f .env.local -- drizzle-kit migrate",
    "db:studio": "dotenvx run -f .env.development.local -f .env.local -- drizzle-kit studio",
    "format": "bun run --bun prettier --write .",
    "format:check": "bun run --bun prettier --check .",
    "typecheck": "bun run --bun tsc --noEmit",
    "prepare": "husky",
    "test": "bun run --bun vitest",
    "test:run": "bun run --bun vitest run",
    "test:ui": "bun run --bun vitest --ui",
    "setup": "bun x vercel link"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.23.3",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@rwh/keystrokes": "^1.5.6",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/leaflet": "^1.9.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.2",
    "gpxparser": "^3.0.8",
    "human-id": "^4.1.1",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet-geosearch": "^4.2.0",
    "lucide-react": "^0.518.0",
    "next": "15.3.4",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-use-promise": "^0.5.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.45.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.4",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
